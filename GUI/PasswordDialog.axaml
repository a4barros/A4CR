<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="180"
        x:Class="GUI.PasswordDialog"
        Height="180"
        Width="600"
        CanMaximize="False"
        CanResize="False"
        Title="PasswordDialog"
        WindowStartupLocation="CenterOwner">
  <StackPanel Margin="30">
	  <Label>Please enter the password</Label>
      <TextBox x:Name="PasswordTextbox" PasswordChar="*"/>
      <Button Margin="0 20" HorizontalAlignment="Right" Content="OK" Click="ButtonOK_Click"/>
  </StackPanel>
</Window>
